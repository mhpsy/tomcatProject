<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <context-param>
        <param-name>username</param-name>
        <param-value>context</param-value>
    </context-param>

    <servlet>
        <servlet-name>path1</servlet-name>
        <servlet-class>top.mhpsy.webServlet.servletConfig_1</servlet-class>
        <init-param>
            <param-name>username</param-name>
            <param-value>init</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>path1</servlet-name>
        <url-pattern>/path1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>path2</servlet-name>
        <servlet-class>top.mhpsy.webServlet.servletConfig_2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>path2</servlet-name>
        <url-pattern>/path2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>path3</servlet-name>
        <servlet-class>top.mhpsy.webServlet.servletConfig_3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>path3</servlet-name>
        <url-pattern>/path3</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>path4</servlet-name>
        <servlet-class>top.mhpsy.webServlet.servletConfig_4</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>path4</servlet-name>
        <url-pattern>/path4</url-pattern>
    </servlet-mapping>
</web-app>